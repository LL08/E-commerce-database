<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="SeckillMapper">
	<!-- 根据id查询一个订单明细表信息 -->
	<select id="selectSeckillById" parameterType="int" resultType="pojo.Seckill">
		select * from sec_kill where seckillID = #{seckillID}
	</select>
</mapper>